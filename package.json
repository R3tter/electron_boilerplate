{
  "name": "electron_boilerplate",
  "version": "0.0.1",
  "description": "electron_boilerplate",
  "main": "./dist/main.js",
  "scripts": {
    "test": "jest --watchAll=false",
    "webpack:dev": "webpack serve --config ./config/webpack.renderer.dev.js & webpack --config ./config/webpack.main.dev.js",
    "webpack:prod": "rm -rf ./dist/* && webpack --config ./config/webpack.prod.js",
    "dev": "electron dev/main.js",
    "build": "electron-builder -m"
  },
  "build": {
    "appId": "your.id",
    "directories": {
      "buildResources": "dist",
      "output": "out"
    },
    "nsis": {
      "packElevateHelper": false
    },
    "mac": {
      "icon": "icons/mac/icon.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "icon": "icons/win/icon.ico",
      "target": [
        {
          "target": "NSIS",
          "arch": "x64"
        }
      ]
    },
    "linux": {
      "icon": "icons/linux/icon.png",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/R3tter/webpack_boilerplate.git"
  },
  "keywords": [],
  "author": "electron_boilerplate",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/R3tter/webpack_boilerplate/issues"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ],
    "transform": {
      "^.+\\.(jsx|js)$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  },
  "homepage": "https://gitlab.com/R3tter/webpack_boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@svgr/webpack": "^5.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "css-loader": "^5.0.1",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@jmind.systems/react-modal": "0.0.27",
    "@jmind.systems/react-status-controller": "0.0.14",
    "aphrodite": "^2.4.0",
    "api-req-redux": "2.0.10",
    "core-js": "^3.9.1",
    "i18next": "^20.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.12",
    "react-notify-library": "0.0.14",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "reset-css": "^5.0.1",
    "use-form-validation-hook": "^1.0.19"
  }
}
